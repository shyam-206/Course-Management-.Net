</head>
<body>
    
    <nav class="navbar navbar-light bg-dark">
        <div class="container-fluid">
            <span class="navbar-text h3 text-white">
                Course Management
            </span>
            <div class="d-flex align-content-center mt-3 gap-2">
                <p class="text-white">@Session["Useremail"]</p>
                <a href="/Login/Logout" class="text-white text-decoration-none">Logout</a>
            </div>
            
        </div>
    </nav>
    <div class="body-content">
        @RenderBody()
        
    </div>

    @Scripts.Render("~/bundles/jquery")
    
   
